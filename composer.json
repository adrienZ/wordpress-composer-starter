{
    "name": "adrienzaganelli/zogzog",
    "authors": [
        {
            "name": "AdrienZ",
            "email": "adrienzaganelli@gmail.com"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "extra": {
        "installer-paths": {
            "wp-content/mu-plugins/{$name}": [
